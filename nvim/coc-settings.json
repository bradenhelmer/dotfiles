{
    "languageserver": {
        "ccls": {
            "command": "ccls",
            "filetypes": [
                "c",
                "cc",
                "cu",
                "cpp",
                "c++",
                "objc",
                "objcpp",
                "h",
                "inc"
            ],
            "rootPatterns": [
                ".ccls",
                "compile_commands.json",
                ".git/",
                ".hg/"
            ],
            "initializationOptions": {
                "cache": {
                    "directory": "/tmp/ccls"
                }
            }
        },
        "tblgen": {
            "command": "~/dev/tools/llvm-project/build/bin/tblgen-lsp-server",
            "args": [
                "--tablegen-compilation-database=~/dev/tools/llvm-project/build/tablegen_compile_commands.yml",
                "--tablegen-extra-dir=~/dev/tools/llvm-project/mlir/include/mlir"
            ],
            "filetypes": [
                "td"
            ]
        }
    },
    "snippets.ultisnips.pythonPrompt": false,
    "java.jdt.ls.java.home": "/usr/lib/jvm/java-17-openjdk-amd64",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-17",
            "path": "/usr/lib/jvm/java-17-openjdk-amd64",
            "default": true
        }
    ],
    "java.completion.guessMethodArguments": false,
    "inlayHint.enableParameter": false
}
